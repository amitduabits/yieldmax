{
  "name": "yieldmax",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:e2e": "playwright test",
    "// Demo Scripts": "",
    "demo:setup": "node scripts/demo/setup-demo.js",
    "demo:spike": "node scripts/demo/trigger-spike.js",
    "demo:monitor": "node scripts/demo/monitor-demo.js",
    "demo:run": "npm run demo:setup && npm run dev",
    "// Contract Scripts": "",
    "contracts:compile": "cd contracts && npx hardhat compile",
    "contracts:deploy": "cd contracts && npx hardhat run scripts/deploy.js --network sepolia",
    "contracts:deploy:ai": "cd contracts && npx hardhat run scripts/deploy-ai-contracts.js --network sepolia",
    "contracts:verify": "cd contracts && npx hardhat verify",
    "// Development": "",
    "clean": "rm -rf .next node_modules",
    "reinstall": "npm run clean && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^1.3.0",
    "@tanstack/react-query": "^5.0.0",
    "ethers": "^5.7.2",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.292.0",
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.3",
    "recharts": "^2.9.0",
    "styled-components": "^6.1.0",
    "viem": "^1.19.0",
    "wagmi": "^1.4.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^5.3.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.30",
    "chai": "^5.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.24.2",
    "typescript": "^5.0.0"
  }
